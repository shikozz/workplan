<Page x:Class="WorkPlan.Pages.Structure"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WorkPlan.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Structure">

    <Grid x:Name="structure" Background="AliceBlue">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Width="100" HorizontalAlignment="Right" Content="Назад" x:Name="backButton" Margin="8" Click="backButton_Click"/>
        <StackPanel Grid.Row="1" x:Name="stackTest" HorizontalAlignment="Center">

        </StackPanel>
    </Grid>
</Page>
